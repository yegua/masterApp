#chart-wrapper{
	width: 100%;
	height: 100px;
	overflow-x: scroll;
}
.chart-container{
	height: 100px;
	position: relative;
}
/*tab切换样式*/
.tab-lists,.balance-lists{
	width: 100%;
	padding: 10px 8% 18px 8%;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
}
.tab-lists li ,.balance-lists li{
	color: #999;
	width: 45%;
	padding-bottom: 12px;
	border-bottom: 2px solid #fff; 
}
.tab-lists li span,.balance-lists li span{
	display: inline-block;
	width: 2.8em;
	height: 2.8em;	
}
.tab-lists li span.outer-bg{
	background: url(../images/outer-icon.png) no-repeat 50%;
	background-size: 100% 100%;
}
.tab-lists li span.inner-bg{
	background: url(../images/inner-icon.png) no-repeat 50%;
	background-size: 100% 100%;
}
/*加盟账单tab样式*/
.balance-lists li span.yue-bg{
	background: url(../images/icon-yue.png) no-repeat 50%;
	background-size: 100% 100%;
}
.balance-lists li span.chongzhi-bg{
	background: url(../images/icon-pay.png) no-repeat 50%;
	background-size: 100% 100%;
}
.tab-lists li span.outerselected{
	background: url(../images/outer-icon-active.png) no-repeat 50%;
	background-size: 100% 100%;
}
.tab-lists li span.innerselected{
	background: url(../images/inner-icon-active.png) no-repeat 50%;
	background-size: 100% 100%;
}
.tab-lists li em{
	font-size: 1.4em;
	margin-left: 6px;
}
.tab-lists li.active{
	border-color: #0099ff;
	color: #0099ff;
}
/*货量统计重量分析切换的两个内容页*/
.contents-tabs{
	width: 100%;
	padding-top: 10px;
	position: relative;
}
.contents-tabs .content-lists{
	width: 100%;
	display: none;
	position: absolute;
	left: 0;
	top: 0;
}
.contents-tabs .content-lists.show{
	display: block;
}
.contents-tabs .content-lists.hide{
	display: none;
}
/*前三天数据展示圈*/
.totalData{
	width: 100%;
	padding-bottom: 12px;
}
.totalData dl{
	width: 33%;
	text-align: center;
	background: url(../images/border-right.png) no-repeat top right;
	background-size: 1px 70%;
}
.totalData dl:nth-child(3){
	background: none;
}
.totalData dl dt{
	width: 5.6em;
	height: 5.6em;
	border-radius: 100%;
	color: #fff;
	text-align: center;
	line-height: 5.6em;
	margin: 0 auto;
}
.totalData dl dd{
	text-align: center;
	margin-top: 6px;
	font-weight: 600;
}
.totalData dl dt.bg-blue{
	background: #38dab9;
}
.totalData dl dt.bg-orange{
	background: #f8b22d;
}
.totalData dl dt.bg-red{
	background: #f25b4a;
}
/*时间显示区域*/
.date-choose {
	width: 100%;
	padding: 8px 20px;
	background: #eee;
	position: relative;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
}
.date-choose .date-icon{
	padding-right: 26px;	
	background: url(../images/calendar.png) no-repeat right center;
	background-size: 1.5em;
}
.date-choose .date-icon-active{
	padding-left: 26px;
	color: #f25b4a;
	background: url(../images/calendar.png) no-repeat left center;
	background-size: 1.5em;
}
.date-choose .date-activeno {
	padding-right: 26px;
	color: #8b8b8b;
	background: url(../images/icon-gray-calendar.png) no-repeat right center;
	background-size: 1.5em;
}
/*起始日期结束日期选择框*/
.picker-box{
	width: 70%;
	position: absolute;
	height: 100px;
	right: 20px;
	top: 30px;
	display: none;
	background-color: #fff;
	border-radius: 5px;
	z-index: 999;
	-webkit-transform:translateZ(0);
	transform:translateZ(0);
}
.picker-box em{
	display: block;
    width:0;
    height:0;
    border-width:0 7px 5px;
    position: absolute;
    top: -5px;
    right: 14px;
    border-style: solid dashed dashed;
    border-color: transparent transparent #fff;
    font-size: 0; 
    line-height: 0;
}
.picker-box .date-info{
	width: 100%;
	padding: 6px;
	text-align: center;
	box-sizing: border-box;
}
.picker-box .date-info input{
	width: 43%;
	height: 40px;
	padding: 0;
	text-align: center;
	color: #79c0f2;
	border: none;
	margin-bottom: 0;
	border-bottom: 1px solid #79c0f2;
}
.picker-box .date-info input.picker-disable{		
	color: #ccc;
	border-bottom: 1px solid #ccc;
}	
.picker-box .date-confirm {
	width: 100%;
	text-align: right;
	padding: 8px 20px 8px 0;
	box-sizing: border-box;
}
.picker-box .date-confirm .btn-confirm{
	background: #79c0f2;
	padding: 5px 12px;
	color: #fff;
	border-radius: 5px;
}
.data-title{
	width: 92%;
	margin: 0 auto;
	padding: 8px 0;
	font-size: 1.3em;
	font-weight: 600;
	border-bottom: 1px solid #959595;
}
.data-title .change-icon{
	padding-left: 26px;
	background: url(../images/icon-change.png) no-repeat left center;
	background-size: 1.2em;
}
/*表格显示区域*/
#ui-chart-pie,#ui-chart-website{
	width: 100%;
	height: 280px;
	margin-top: 20px;
}
#ui-chart-website {
	margin-bottom: 10px;
}
/*点击图标显示表格数据*/
.more-data {
	width: 100%;
	padding: 15px 0 20px 0;
	background: url(../images/icon-more.png) no-repeat 50% 60%;
	background-size: 1.8em;
}
.more-data.up{
	background: url(../images/icon-more-up.png) no-repeat 50% 60%;
	background-size: 1.8em;
}
/*表格数据显示*/
.table-responsive{
	width: 100%;
	padding: 10px 0 30px 0;
	display: none;
}
.table-responsive .table{
	width: 92%;
	border-spacing: 0;
    border-collapse: collapse;
    margin: 0 auto;
}
.table-responsive .table tr{
	width: 100%;
	border-bottom: 1px solid #c0c0c0;
}
.table-responsive .table tr th,.table-responsive .table tr td{
	width: 33%;
	padding: 10px 0;
	text-align: center;
	font-size: 1.3em;
}
.table-responsive .table tr .alignLeft {
	text-align: left;
	width: 20%;
}
.table-responsive .table tr .alignCenter {
	width: 60%;
}
.table-responsive .table tr .alignRight {
	width: 20%;	
}
.table-responsive .table tr td{
	color: #959595;
}
/*派送时效表格样式*/
.ps-list{
	width: 92%;
	padding-bottom: 10px;
	margin: 0 auto;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
}
.ps-list li {
	width: 100%;
	padding: 10px 0;
	border-top: 1px solid #959595;
}
.ps-list li:nth-child(1){
	border-top: none;
}
.ps-list li span{
	display: inline-block;
	width: 50%;
	font-size: 1.3em;
	padding-left: 25px;
	font-weight: 600;
	border-right: 1px solid #9e9e9e;
}
.ps-list li span:nth-child(1) em:nth-child(1){
	text-decoration: underline;
}
.ps-list li span em{
	color: #ff3e00;
}
.ps-list li span:nth-child(2){
	border-right: none;
}

.none{
	display:none;
}

/*重量段分析*/
.contents-website,.contents-clients{
	top: 13em;
}
.tab-lists li span.website-bg{
	background: url(../images/icon-website.png) no-repeat 50%;
	background-size: 100% 100%;
}
.tab-lists li span.client-bg{
	background: url(../images/icon-person.png) no-repeat 50%;
	background-size: 100% 100%;
}
.tab-lists li span.websiteSelected{
	background: url(../images/icon-website-active.png) no-repeat 50%;
	background-size: 100% 100%;
}
.tab-lists li span.clientSelected{
	background: url(../images/icon-person-active.png) no-repeat 50%;
	background-size: 100% 100%;
}
.select-wrapper {
	width: 100%;
	position: relative;
}
.select-box {
	width: 46%;
	background: #fff;
	font-size: 14px;
	padding: 0 0 0 8px;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
	border: 1px solid #959595;
	border-radius: 30px;
	z-index: 999;
}
.select-box input{
	width: 80%;
	z-index: 1000;
	border: none;
	height: 23px;
	margin: 0;
	padding: 0;
	/*background: #fff;*/
}
.select-box em{
	display: inline-block;
	padding: 10px;
	background: #fff url(../images/icon-search.png) no-repeat center center;
	background-size: 1.2em;
}
/*下拉框样式*/
.select-contents {
	width: 34%;
	border-radius: 5px;
	padding: 5px;
	position: absolute;
	display: none;
	left: 8%;
	top: 43px;
	height: 160px;
	overflow-y: auto;
	background: #fff;
	z-index: 999;
}
/*向上*/
.triangle_border_up{
    width:0;
    height:0;
    border-width:0 7px 5px;
    border-style:solid;
    border-color:transparent transparent #fff;/*透明 透明  灰*/
    position:absolute;
    left: 33%;
    top: 38px;
    z-index: 999;
    display: none;
}
.select-contents li {
	width: 100%;
	text-align: center;
	padding: 6px 0;
}
.select-contents li.selected{
	color: #0099ff;
	border-bottom: 1px solid #0099FF;
}

/*营业额样式*/
.total-results{
    font-size: 26px;
    font-weight: bolder;
    color: white;
    padding: 20px 0;
    text-align: center;
    background: #0099ff;
}
.ui-data-subTitle{
    padding: 5px 0px 20px 0px;
    text-align: center;
    font-size: 16px;
    font-weight: 500;
}
.sumBox{
	width: 94%;
	height: 3em;
	z-index: 99;
	position: fixed;
	left: 3%;
	bottom: 0;
	text-align: center;
	line-height: 3em;
	font-size: 1.2em;
	border-top: 1px solid #d5d5d5;
}
.sumBox span{
	color: #f25b4a;
	font-size: 1.3em;
}
.ui-list {
	padding-top: 10px;
	height: 63%;
	overflow-y: scroll;
}

.ui-list li {
	width: 48%;
    text-align: center;
}

.ui-list-title {
	font-size: 20px;
	padding: 6px 0 10px 0;
    font-weight: 500;
}
.ui-list-title .icon{
	display: inline-block;
	font-size: 1.4em;	
	width: 46px;
	height: 46px;
	line-height: 46px;
	border-radius: 100%;
	margin: 0 10px;
	color:#fff;
	font-weight: 400;
	-webkit-transition: font-size 0.25s ease-out 0s;
	-moz-transition: font-size 0.25s ease-out 0s;
	transition: font-size 0.25s ease-out 0s;
}
.ui-list-title:nth-child(1) i{
	/*background: url(../images/icon-receive.png) no-repeat 20% 32%;
	background-size: 1.5em;*/
	background: #fea700;
}
.ui-list-title:nth-child(2) i{
	/*background: url(../images/icon-cost.png) no-repeat 20% 32%;
	background-size: 1.5em;*/
	background: #f25b4a;
}
.ui-list-subTitle {
	color: #181818;
    padding: 4px;
    font-size: 1.2em;
}

.ui-list-content {
	padding: 4px;
	margin-bottom: 8px;
	font-size: 1.2em;
}

.ui-list li:nth-child(even) {
	border-left: 1px solid #CCC9C9;
}

/*预付款样式*/
.balance-lists {
	padding: 15px 0;
}
.balance-lists li{
	padding: 0;
}
.balance-lists li:nth-child(1){
	width: 55%;
	border-right: 1px solid #666;
}
.balance-lists li:nth-child(2){
	width: 45%;
}
.balance-lists li span{
	margin-right: 10px;
}
.balance-lists li em{
	font-size: 1.3em;
	color: #181818;
}
.bill-contents ul {
	width: 100%;
	padding: 0 20px;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
}
.bill-contents ul li{
	width: 100%;
	padding: 10px 0;
	background: url(../images/border-bottom.png) no-repeat right bottom;	
	background-size: 90% 1px;
}
.bill-contents ul li:last-child{
	background: none;
}
.bill-contents .change-icon em.done{
	color: #f25b4a;
}
.bill-contents .change-icon em.nodone{
	color: #1e90ff;
}
.bill-contents .bill-title{
	border: none;
	background: url(../images/border-bottom.png) no-repeat right bottom;	
	background-size: 90% 1px;
}
.bill-contents .data-title{
	font-size: 1.2em;
	font-weight: 600;
}
.bill-contents ul li>div{
	width: 30%;
	font-size: 1.2em;
}
.bill-contents ul li>img{
	width: 32px;
	height: 32px;
	margin-right: 10px;
}
.left-align{
	text-align: left;
}
.center-align{
	text-align: center;
}
.right-align{
	text-align: right;
}
.test-pay{
	width: 70%;
	height: 100px;
	display: none;
	position: absolute;
	left: 0;
	right: 0;
	bottom: 0;
	top: 0;
	margin: auto;
	text-align: center;
	z-index: 1000;
	background: #fff;
}

/*业绩排名样式*/
#ui-chart-yeji{
	width: 96%;
	height: 400px;
	margin: 10px auto;
	border-bottom: 1px solid #161616;
}

/*物料库存样式*/
.ui-wuliaokc-container {
	color: gray;	
}

.ui-wuliaokc-toolbar {
	/*position: fixed;*/
	width: 100%;
	background-color: white;
	font-size: 1.3em;
}
.ui-wuliaokc-search{
	text-align: left;
	width: 100%;
	padding: 20px 15px;
	z-index: 1;
}
.ui-wuliaokc-search input {
	border: 1px solid grey;
	background-color: white;
	width: 45%;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
	height: 28px;
	line-height: 28px;
	padding: 0 38px 0 8px;
	border-radius: 30px;
	font-size: 12px;
	background: url(../images/icon-search.png) no-repeat 90% 50%;
	background-size: 1.3em;
}
.ui-wuliaokc-countNumLabel {
	padding: 10px 15px;
	font-size: 16px;
	color: #161616;
}

.ui-wuliaokc-list {	
	background: #fff;
}

.ui-wuliaokc-category {
	padding: 2px 15px;
	background-color: #eee;
	color: #999;
	font-size: 1.2em;
	
}

.ui-wuliaokc-product {
	font-size: 16px;
	padding: 20px 10px;
	border-bottom: 1px solid gainsboro;
}

.ui-wuliaokc-product:last-child {
	border-bottom: none;
}

.ui-wuliaokc-name {
	display: inline-block;
	width: 50%;
}

.ui-wuliaokc-num {
	display: inline-block;
	width: 46%;
	color: black;
	font-weight: bolder;
	text-align: right;
}
/*媒体查询iphone4/4s单独处理*/
@media only screen and (width: 320px) {
	.ui-list {		
		height: 55%;
	}
	.ui-list-title .icon{
		width: 40px;
		height: 40px;
		line-height: 40px;
		font-size: 1.3em;
	}
}

/*媒体查询iphone6/6plus/ipad单独处理*/
@media only screen and (min-width: 375px) {
	 .ui-list {		
		height: 66%;		
	}
}